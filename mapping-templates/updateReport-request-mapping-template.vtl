{
    "version": "2017-02-28",
    "operation": "UpdateItem",
    "key": {
        "Id": $util.dynamodb.toDynamoDBJson($ctx.args.Id)
    },
    "update": {
        "expression": "SET DownloadUrl = :downloadUrl",
        "expressionValues": {
            ":downloadUrl": $util.dynamodb.toDynamoDBJson($util.defaultIfNull(${ctx.args.DownloadUrl}, "")),
        }
    }
}